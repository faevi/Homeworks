@model IEnumerable<FirstWebApplication.Models.DataModel>

<h2>Данные</h2>
<p><a asp-action="BackToIndex">Вернуться</a></p>
<table class="table">
    <tr><th>Id</th><th>Data</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Data</td>
        </tr>
    }
</table>